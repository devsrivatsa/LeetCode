class Solution:
    def checkInclusion(self, s1: str, s2: str) -> bool:
        if len(s2) < len(s1):
            return False
        l,r = 0, len(s1)
        hm1, hm2 = collections.Counter(s1), collections.Counter(s2[:len(s1)])
        while r < len(s2):
            if hm1 == hm2: return True
            hm2[s2[l]] -= 1
            hm2[s2[r]] = hm2.get(s2[r], 0) + 1
            l+=1
            r+=1
        return hm1 == hm2
