class Solution:
    def minSubArrayLen(self, target: int, nums: List[int]) -> int:
        min_win = float("inf")
        curr_sum,l,r = 0,0,0
        while r < len(nums):
            # r+=1
            curr_sum += nums[r]
            while curr_sum >= target:
                min_win = min(min_win, r-l+1)
                curr_sum -= nums[l]
                l+=1
            r+=1
        return 0 if min_win == float("inf") else min_win
