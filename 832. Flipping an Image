class Solution:
    def flipAndInvertImage(self, A: List[List[int]]) -> List[List[int]]:
        newA = []
        for a in A:
            newa=[]
            for i in range((len(a)-1), -1, -1):
                if a[i]==0:
                    z = 1
                else:
                    z = 0
                newa.append(z)
            newA.append(newa)
        return newA
                
                
