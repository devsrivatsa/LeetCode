# Definition for a binary tree node.
# class TreeNode:
#     def __init__(self, x):
#         self.val = x
#         self.left = None
#         self.right = None

class Solution:
    count = 0
    def rangeSumBST(self, root, L, R):
        if root:
            if L <= root.val <= R:
                self.count+=root.val
            self.rangeSumBST(root.left,L,R)
            self.rangeSumBST(root.right,L,R)
        
        return self.count
        
