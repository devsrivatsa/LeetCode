class Solution:
    
    def climbStairs(self, n: int) -> int:
        dp_arr = [-1 for i in range(n+1)]
        def dp_memo(n: int) -> int:
            if n <= 1:
                return 1
            if dp_arr[n] != -1:
                return dp_arr[n]
            dp_arr[n] = dp_helper(n-1) + dp_helper(n-2)
            return dp_arr[n]
        return dp_helper(n)

        def dp_tab(n:int) -> int:
            if n <=1:
                return 1
            prev1,prev2,curr = 1,1,0
            for i in range(2, n+1):
                curr = prev2 + prev1
                prev2 = prev1
                prev1 = curr
            return curr
        


        
